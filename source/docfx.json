{
  "build": {
    "sitemap": {
        "baseurl": "https://aristurtle.net",
        "priority": 0.1,
        "changefreq": "daily"
    },
    "content": [
      {
        "files": [
          "games/**.md",
          "games/**/toc.yml",
          "tutorials/**.md",
          "tutorials/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "../artifacts/_site",
    "globalMetadata": {
      "_appTitle": "Aristurtle",
      "_appName": "Aristurtle",
      "_appLogoPath": "images/logo.svg",
      "_appFooter": "Â© 2023 Christopher Whitley",
      "_description": "Official site of all Aristurtle related things",
      "_author": "Christopher Whitley",
      "_ogImage": "aristurtle.png"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "templates/aristurtle"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": true
  }
}
